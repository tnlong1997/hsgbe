{
  "name": "hsgexpress",
  "description": "Some mistakes you can't make twice.",
  "author": "Yasuo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "js-lint": "./node_modules/.bin/eslint '**/*.js'",
    "js-lint-fix": "./node_modules/.bin/eslint '**/*.js' --fix",
    "lint": "npm run js-lint",
    "js-lint-win": "./node_modules/.bin/eslint **/*.js",
    "js-lint-win-fix": "./node_modules/.bin/eslint **/*.js --fix",
    "lint-win": "npm run js-lint-win && npm run html-lint-win && npm run css-lint-win"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.2",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "nodemon": "^1.18.11",
    "redoc-cli": "^0.8.4"
  }
}
